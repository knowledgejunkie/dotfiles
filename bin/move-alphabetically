#!/bin/bash

mkdir -p 0-9 2>/dev/null
mkdir -p A-E/{A..E} 2>/dev/null
mkdir -p F-J/{F..J} 2>/dev/null
mkdir -p K-O/{K..O} 2>/dev/null
mkdir -p P-S/{P..S} 2>/dev/null
mkdir -p T-Z/{T..Z} 2>/dev/null

mv [0-9]* 0-9/ 2>/dev/null

# Leave "A" and "T" until later
mv B* A-E/B/ 2>/dev/null
mv C* A-E/C/ 2>/dev/null
mv D* A-E/D/ 2>/dev/null
mv E* A-E/E/ 2>/dev/null

mv F* F-J/F/ 2>/dev/null
mv G* F-J/G/ 2>/dev/null
mv H* F-J/H/ 2>/dev/null
mv I* F-J/I/ 2>/dev/null
mv J* F-J/J/ 2>/dev/null

mv K* K-O/K/ 2>/dev/null
mv L* K-O/L/ 2>/dev/null
mv M* K-O/M/ 2>/dev/null
mv N* K-O/N/ 2>/dev/null
mv O* K-O/O/ 2>/dev/null

mv P* P-S/P/ 2>/dev/null
mv Q* P-S/Q/ 2>/dev/null
mv R* P-S/R/ 2>/dev/null
mv S* P-S/S/ 2>/dev/null

mv U* T-Z/U/ 2>/dev/null
mv V* T-Z/V/ 2>/dev/null
mv W* T-Z/W/ 2>/dev/null
mv X* T-Z/X/ 2>/dev/null
mv Y* T-Z/Y/ 2>/dev/null
mv Z* T-Z/Z/ 2>/dev/null

mv The\ [0-9]* 0-9/ 2>/dev/null
mv The\ A* A-E/A/ 2>/dev/null
mv The\ B* A-E/B/ 2>/dev/null
mv The\ C* A-E/C/ 2>/dev/null
mv The\ D* A-E/D/ 2>/dev/null
mv The\ E* A-E/E/ 2>/dev/null
mv The\ F* F-J/F/ 2>/dev/null
mv The\ G* F-J/G/ 2>/dev/null
mv The\ H* F-J/H/ 2>/dev/null
mv The\ I* F-J/I/ 2>/dev/null
mv The\ J* F-J/J/ 2>/dev/null
mv The\ K* K-O/K/ 2>/dev/null
mv The\ L* K-O/L/ 2>/dev/null
mv The\ M* K-O/M/ 2>/dev/null
mv The\ N* K-O/N/ 2>/dev/null
mv The\ O* K-O/O/ 2>/dev/null
mv The\ P* P-S/P/ 2>/dev/null
mv The\ Q* P-S/Q/ 2>/dev/null
mv The\ R* P-S/R/ 2>/dev/null
mv The\ S* P-S/S/ 2>/dev/null
mv The\ T* T-Z/T/ 2>/dev/null
mv The\ U* T-Z/U/ 2>/dev/null
mv The\ V* T-Z/V/ 2>/dev/null
mv The\ W* T-Z/W/ 2>/dev/null
mv The\ X* T-Z/X/ 2>/dev/null
mv The\ Y* T-Z/Y/ 2>/dev/null
mv The\ Z* T-Z/Z/ 2>/dev/null

mv T* T-Z/T/ 2>/dev/null

mv A\ B* A-E/B/ 2>/dev/null
mv A\ C* A-E/C/ 2>/dev/null
mv A\ D* A-E/D/ 2>/dev/null
mv A\ F* F-J/E/ 2>/dev/null
mv A\ G* F-J/G/ 2>/dev/null
mv A\ H* F-J/H/ 2>/dev/null
mv A\ J* F-J/J/ 2>/dev/null
mv A\ K* K-O/K/ 2>/dev/null
mv A\ L* K-O/L/ 2>/dev/null
mv A\ M* K-O/M/ 2>/dev/null
mv A\ N* K-O/N/ 2>/dev/null
mv A\ P* P-S/P/ 2>/dev/null
mv A\ Q* P-S/Q/ 2>/dev/null
mv A\ R* P-S/R/ 2>/dev/null
mv A\ S* P-S/S/ 2>/dev/null
mv A\ T* T-Z/T/ 2>/dev/null
mv A\ V* T-Z/V/ 2>/dev/null
mv A\ W* T-Z/W/ 2>/dev/null
mv A\ X* T-Z/X/ 2>/dev/null
mv A\ Y* T-Z/Y/ 2>/dev/null
mv A\ Z* T-Z/Z/ 2>/dev/null

mv A* A-E/A/ 2>/dev/null
